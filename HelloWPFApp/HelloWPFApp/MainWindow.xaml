        xmlns:local="clr-namespace:HelloWPFApp"
        mc:Ignorable="d"
        Title="DunkyCo GET window" Height="450" Width="800">
    <Grid Background="MediumPurple">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="339*"/>
            <ColumnDefinition Width="61*"/>
        </Grid.ColumnDefinitions>
        <Button Background="Yellow" Content="GET api status" HorizontalAlignment="Left" Height="36" Margin="167,28,0,0" VerticalAlignment="Top" Width="93" Click="API_status_Button_Click"/>
        <Button Background="Yellow" Content="GET all users" HorizontalAlignment="Left" Height="36" Margin="23,28,0,0" VerticalAlignment="Top" Width="94" Click="GET_users_Button_Click"/>

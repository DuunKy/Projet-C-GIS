        <Button Background="Yellow" Content="GET all commands" HorizontalAlignment="Left" Height="36" Margin="306,93,0,0" VerticalAlignment="Top" Width="108" Click="GET_commands_Button_Click"/>
        <Button Background="Yellow" Content="GET all carts" HorizontalAlignment="Left" Height="36" Margin="470,56,0,0" VerticalAlignment="Top" Width="80" Click="GET_carts_Button_Click"/>
        <Button Background="Yellow" Content="POST window" HorizontalAlignment="Left" Height="36" Margin="53,246,0,0" VerticalAlignment="Top" Width="80" Click="POST_window_Button_Click"/>
    </Grid>
</Window>
